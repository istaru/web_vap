{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "typeRoots": ["./node_modules/@types"],
    "declaration": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitUseStrict": false,
    "noImplicitAny": false
  },
  "exclude": ["node_modules"]
}